{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "quotemark": [true, "single", "jsx-double"],
    "no-console": [false],
    "object-literal-sort-keys": false,
    "class-name": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods",
      "ignore-interfaces"
    ],
    "triple-equals": [true, "allow-null-check"],
    "typedef": [true],
    "use-strict": true
  },
  "rulesDirectory": []
}
